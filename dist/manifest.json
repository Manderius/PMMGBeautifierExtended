{
  "manifest_version": 2,
  "name": "PMMG Beautifier",
  "description": "Improve and tweak the interface elements of PrUn website",
  "version": "0.5.0",
  "icons": {
    "128": "icon128.png"
  },
  "browser_action": {
	"default_popup": "popup_firefox.html"
  },
  "permissions": [
    "https://apex.prosperousuniverse.com/*",
	"storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://apex.prosperousuniverse.com/*", "file:///*/test.html"
      ],
      "js": [
        "main.js"
      ]
    }
  ]
}
